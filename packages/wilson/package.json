{
  "name": "wilson",
  "version": "1.0.0-beta.11",
  "license": "MIT",
  "author": "Christoph Werner <christoph@codepunkt.de>",
  "description": "Blazing-fast, opinionated static site generator for Preact",
  "bin": {
    "wilson": "bin/wilson.js"
  },
  "type": "module",
  "exports": {
    ".": "./dist/node/index.js",
    "./package.json": "./package.json",
    "./dist/client/main.js": "./dist/client/main.js",
    "./dist/client/context/prefetch": "./dist/client/context/prefetch.js"
  },
  "types": "dist/node/index.d.ts",
  "files": [
    "bin/**",
    "dist/**",
    "client.d.ts",
    "virtual.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codepunkt/wilson",
    "directory": "packages/wilson"
  },
  "bugs": {
    "url": "https://github.com/codepunkt/wilson/issues"
  },
  "scripts": {
    "wilson": "wilson",
    "build": "yarn run clean && run-p \"build:*\" && yarn run copy:types",
    "build:node": "tsc -p src/node",
    "build:client": "tsc -p src/client",
    "clean": "node -e \"fs.rmSync('dist', { force: true, recursive: true })\"",
    "copy:types": "node -e \"fs.copyFileSync(path.join('src', 'types.d.ts'), path.join('dist', 'types.d.ts'))\"",
    "start": "yarn run clean && run-p \"start:*\"",
    "start:node": "tsc -w -p src/node",
    "start:client": "tsc -w -p src/client",
    "prepack": "yarn run build",
    "test": "jest --coverage --watchAll",
    "test:ci": "jest --coverage",
    "coverage": "serve coverage/lcov-report"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "preact",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "dependencies": {
    "@codepunkt/wasm-layout-text": "^3.0.0",
    "@mapbox/hast-util-to-jsx": "^1.0.0",
    "@preact/preset-vite": "^2.1.0",
    "@svgr/core": "^5.5.0",
    "acorn": "^8.1.1",
    "astring": "^1.7.4",
    "boxen": "^5.0.0",
    "brotli-size": "^4.0.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "estree-walker": "3.0.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "gatsby-remark-vscode": "^3.3.0",
    "gray-matter": "^4.0.2",
    "hast": "^1.0.0",
    "hoofd": "^1.3.1",
    "html-minifier-terser": "^5.1.1",
    "jimp": "^0.16.1",
    "json5": "^2.2.0",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "module-alias": "^2.2.2",
    "node-cache": "^5.1.2",
    "ora": "^5.4.0",
    "plist": "^3.0.2",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "pretty-error": "^3.0.3",
    "readdirp": "^3.6.0",
    "rehype-autolink-headings": "^5.0.1",
    "rehype-parse": "^7.0.1",
    "rehype-raw": "^5.1.0",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-parse": "^10.0.0",
    "remark-rehype": "^9.0.0",
    "remark-stringify": "^10.0.0",
    "rss": "^1.2.2",
    "sass": "^1.36.0",
    "slugify": "^1.6.0",
    "sucrase": "^3.20.0",
    "throttles": "^1.0.1",
    "typescript": "^4.2.4",
    "unified": "^10.0.1",
    "unist-util-is": "^5.1.1",
    "unist-util-visit": "^3.1.0",
    "vite": "2.4.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/estree": "^0.0.50",
    "@types/fs-extra": "^9.0.12",
    "@types/html-minifier-terser": "^5.1.2",
    "@types/jest": "^26.0.24",
    "@types/mime-types": "^2.1.0",
    "@types/minimatch": "^3.0.5",
    "@types/minimist": "^1.2.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.4.2",
    "@types/requestidlecallback": "^0.3.4",
    "@types/rss": "^0.0.29",
    "@types/serve-handler": "^6.1.1",
    "jest": "^27.0.6",
    "serve": "^12.0.0",
    "ts-jest": "^27.0.4",
    "tslib": "^2.2.0"
  }
}
